{
  "author": "hiusnguyen201@gmail.com",
  "type": "module",
  "name": "server-clothes-store",
  "version": "0.0.0",
  "private": true,
  "imports": {
    "#src/*": "./src/*.js",
    "#test/*": "./test/*.js"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./src/server.js ",
    "dev": "cross-env NODE_ENV=development nodemon -L --env-file=env/.env.development ./src/server.js",
    "test": "cross-env NODE_ENV=test jest --config --env-file=env/.env.test",
    "test:silent": "cross-env NODE_ENV=test jest --config --env-file=env/.env.test --silent",
    "test:watch": "cross-env NODE_ENV=test jest --watch --config --env-file=env/.env.test",
    "build": "echo 'No build step required for Express.js'",
    "vercel-build": "npm run build",
    "seed-db:dev": "cross-env NODE_ENV=development node --env-file=env/.env.development ./src/database/seed.database.js",
    "seed-part-db:dev": "cross-env NODE_ENV=development node --env-file=env/.env.development ./src/database/seed-part.database.js",
    "seed-db:prod": "cross-env NODE_ENV=production node --env-file=env/.env.production ./src/database/seed.database.js",
    "initialize-db:dev": "cross-env NODE_ENV=development node --env-file=env/.env.development ./src/database/initialize.database.js",
    "initialize-db:prod": "cross-env NODE_ENV=production node --env-file=env/.env.production ./src/database/initialize.database.js",
    "drop-db:dev": "cross-env NODE_ENV=development  node --env-file=env/.env.development ./src/database/drop.database.js",
    "drop-db:prod": "cross-env NODE_ENV=production  node --env-file=env/.env.production ./src/database/drop.database.js",
    "refresh-db:dev": "npm run drop-db:dev && npm run initialize-db:dev && npm run seed-db:dev",
    "refresh-db:prod": "npm run drop-db:prod && npm run initialize-db:prod && npm run seed-db:prod"
  },
  "dependencies": {
    "@faker-js/faker": "^9.7.0",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.3",
    "axios": "^1.8.3",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.49.1",
    "cloudinary": "^2.5.1",
    "compression": "^1.7.5",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "escape-string-regexp": "^4.0.0",
    "exceljs": "^4.4.0",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "http-errors": "~1.6.3",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.46",
    "mongoose": "^8.8.3",
    "mongoose-sequence": "^6.0.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "ngrok": "^5.0.0-beta.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.16",
    "qrcode": "^1.5.4",
    "react-redux": "^9.2.0",
    "redis": "^4.7.0",
    "sanitize-html": "^2.16.0",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "uuid": "^11.0.3",
    "vietnam-provinces": "^0.0.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.26.0",
    "@types/express": "^4.17.21",
    "@types/mongoose": "^5.11.97",
    "@types/mongoose-sequence": "^3.0.11",
    "@types/node": "^20.10.5",
    "@types/sanitize-html": "^2.15.0",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "ts-jest": "^29.3.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
