{
  "author": "hiusnguyen201@gmail.com",
  "type": "module",
  "name": "server-clothes-store",
  "version": "0.0.0",
  "private": true,
  "imports": {
    "#src/*": "./src/*.js",
    "#app/*": "./src/app/*.js",
    "#models/*": "./src/models/*.js",
    "#core/*": "./src/core/*.js",
    "#config/*": "./src/config/*.js",
    "#database/*": "./src/database/*.js",
    "#middlewares/*": "./src/middlewares/*.js",
    "#modules/*": "./src/modules/*.js",
    "#routers/*": "./src/routers/*.js",
    "#utils/*": "./src/utils/*.js"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./src/main.js",
    "dev": "cross-env NODE_ENV=development nodemon ./src/main.js",
    "test": "cross-env NODE_ENV=test jest --config",
    "test:silent": "cross-env NODE_ENV=test jest --config --silent",
    "test:watch": "cross-env NODE_ENV=test jest --watch --config",
    "build": "echo 'No build step required for Express.js'",
    "vercel-build": "npm run build",
    "seed": "cross-env NODE_ENV=development node ./src/database/seeders/seed-data.js"
  },
  "dependencies": {
    "@faker-js/faker": "^9.3.0",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.5.1",
    "compression": "^1.7.5",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "http-errors": "~1.6.3",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.46",
    "mongoose": "^8.8.3",
    "mongoose-delete": "^1.0.2",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.16",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "streamifier": "^0.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  }
