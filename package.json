{
  "author": "hiusnguyen201@gmail.com",
  "type": "module",
  "name": "server-clothes-store",
  "version": "0.0.0",
  "private": true,
  "imports": {
    "#src/*": "./src/*.js"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production  node --env-file=env/.env.production ./src/main.js ",
    "dev": "cross-env NODE_ENV=development nodemon --env-file=env/.env.development ./src/main.js",
    "test": "cross-env NODE_ENV=test jest --config --env-file=env/.env.test",
    "test:silent": "cross-env NODE_ENV=test jest --config --silent",
    "test:watch": "cross-env NODE_ENV=test jest --watch --config",
    "build": "echo 'No build step required for Express.js'",
    "vercel-build": "npm run build",
    "seed:dev": "cross-env NODE_ENV=development node --env-file=env/.env.development ./src/database/seeders/seed-data.js",
    "seed:prod": "cross-env NODE_ENV=production node --env-file=env/.env.production ./src/database/seeders/seed-data.js",
    "drop-db": "cross-env NODE_ENV=development  node --env-file=env/.env.development ./src/database/drop-database.js"
  },
  "dependencies": {
    "@faker-js/faker": "^9.3.0",
    "axios": "^1.8.3",
    "bcrypt": "^5.1.1",
    "chalk": "^5.4.1",
    "cloudinary": "^2.5.1",
    "compression": "^1.7.5",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "http-errors": "~1.6.3",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.46",
    "mongoose": "^8.8.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.16",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  }
}
